<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="bean1" class="com.study.spring.sample.di.Bean1" scope="prototype" >
		<property name="b2" ref="bean2"></property>
	</bean>
	
	<bean id="bean2" class="com.study.spring.sample.di.Bean2" scope="prototype" >
		<property name="b1" ref="bean1"></property>
	</bean>
	
</beans>
	
	
	